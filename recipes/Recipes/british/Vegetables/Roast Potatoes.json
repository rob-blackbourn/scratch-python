  "recipeIngredient": [
    "2 tbsp fat (preferably duck fat),",
    "2 medium size potatoes (Maris Piper or King Edward),",
    "2 cloves of garlic,",
    "1 sprig thyme,",
    "semolina flour,",
    "salt."
  ],
  "recipeInstructions": [
    "1. Preheat the oven to 190C/375F/Gas 5.",
    "2. Parboil the potatoes for 8 minutes.",
    "3. Drain, and shake until slightly rough on the outside.",
    "4. Dust with semolina flour.",
    "5. Put the fat in the baking tray, and leave in the oven for 5 minutes.",
    "6. Roast for 45 minutes to 1 hour, basting occasionally."
  ],
